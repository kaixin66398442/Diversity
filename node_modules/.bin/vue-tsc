#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/桌面/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@1.4.2_typescript@5.0.2/node_modules/vue-tsc/bin/node_modules:/mnt/d/桌面/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@1.4.2_typescript@5.0.2/node_modules/vue-tsc/node_modules:/mnt/d/桌面/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@1.4.2_typescript@5.0.2/node_modules:/mnt/d/桌面/Diversity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/桌面/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@1.4.2_typescript@5.0.2/node_modules/vue-tsc/bin/node_modules:/mnt/d/桌面/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@1.4.2_typescript@5.0.2/node_modules/vue-tsc/node_modules:/mnt/d/桌面/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@1.4.2_typescript@5.0.2/node_modules:/mnt/d/桌面/Diversity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
fi
