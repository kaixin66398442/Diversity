#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+webpack@5.83.1/node_modules/webpack/bin/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+webpack@5.83.1/node_modules/webpack/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+webpack@5.83.1/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+webpack@5.83.1/node_modules/webpack/bin/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+webpack@5.83.1/node_modules/webpack/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+webpack@5.83.1/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+webpack@5.83.1/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+webpack@5.83.1/node_modules/webpack/bin/webpack.js" "$@"
fi
