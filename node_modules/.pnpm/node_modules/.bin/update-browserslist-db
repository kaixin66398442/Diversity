#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.0.11_browserslist@4.21.5/node_modules/update-browserslist-db/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.0.11_browserslist@4.21.5/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.0.11_browserslist@4.21.5/node_modules/update-browserslist-db/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.0.11_browserslist@4.21.5/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../update-browserslist-db/cli.js" "$@"
fi
