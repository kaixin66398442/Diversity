#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules/vue-demi/bin/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules/vue-demi/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules/vue-demi/bin/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules/vue-demi/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules:/mnt/d/桌面/Diversity低代码项目/Diversity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+vue-demi@0.14.5_vue@3.2.47/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
fi
